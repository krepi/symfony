{% extends 'base.html.twig' %}


{% block body %}
    {% if(messages|length > 0) %}
        {% for key, message in messages|slice(0,limit) %}

            <a href="{{ path('show-one',{id:key}) }}"><h1>{{ message.message }}</h1></a>
     {{ include('hello/_message.html.twig',{message:message}) }}
        {% endfor %}
    {% else %}
        <p>There is nothing here yet!</p>
    {% endif %}
{% endblock %}

